    "## 2.3.1 Задание"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 23,
   "id": "c946b98d",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "6\n"
     ]
    }
   ],
   "source": [
    "def euclid(a,b):\n",
    "    assert len(a) == len(b)\n",
    "    length = 0\n",
    "    for i in range(len(a)):\n",
    "        length += (a[i] - b[i])**2\n",
    "    return length\n",
    "\n",
    "\n",
    "a = pd.Series([2,3,6,4])\n",
    "b = pd.Series([1,3,5,6])\n",
    "print(euclid(a,b))"
   ]
  },
